    "@types/stream-throttle": "^0.1.1",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.34.0",
    "lint-staged": "^13.1.2",
    "simple-git-hooks": "^2.8.1",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "vite": "^4.1.1"
  },
  "simple-git-hooks": {
