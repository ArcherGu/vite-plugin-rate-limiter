{
  "name": "vite-plugin-rate-limiter",
  "version": "0.0.0",
  "packageManager": "pnpm@8.6.1",
  "description": "Limit the response rate of ViteDevServer",
  "author": "ArcherGu",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/ArcherGu/vite-plugin-rate-limiter.git"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "rate",
    "stream"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "vite": ">=2.0.0"
  },
  "dependencies": {
    "stream-throttle": "^0.1.3"
  },
  "devDependencies": {
    "@lightwing/eslint-config": "^0.0.16",
    "@types/node": "^18.15.11",
    "@types/stream-throttle": "^0.1.1",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.38.0",
    "lint-staged": "^13.2.1",
    "simple-git-hooks": "^2.8.1",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "vite": "^4.2.1",
    "vitest": "^0.32.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md,json,yml}": [
      "eslint --fix"
    ]
  }
}
