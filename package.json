{
  "name": "vite-plugin-rate-limiter",
  "version": "0.0.0",
  "packageManager": "pnpm@10.3.0",
  "description": "Limit the response rate of ViteDevServer",
  "author": "ArcherGu",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/ArcherGu/vite-plugin-rate-limiter.git"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "rate",
    "stream"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "vite": ">=2.0.0"
  },
  "dependencies": {
    "stream-throttle": "^0.1.3"
  },
  "devDependencies": {
    "@lightwing/eslint-config": "^1.0.102",
    "@types/node": "^22.12.0",
    "@types/stream-throttle": "^0.1.4",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.19.0",
    "lint-staged": "^15.4.3",
    "simple-git-hooks": "^2.11.1",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "vite": "^6.0.11",
    "vitest": "^3.0.4"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md,json,yml}": [
      "eslint --fix"
    ]
  }
}
